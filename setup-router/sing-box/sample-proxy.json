{
  "log": {
    "disabled": false,
    // trace debug info warn error fatal panic
    "level": "error",
    // "output": "/var/log/vbox/box.log",
    "timestamp": true
  },
  "dns": {
    // When proxy server support ipv4 only
    // "strategy": "prefer_ipv4", // prefer_ipv4,prefer_ipv6,ipv4_only,ipv6_only
    "servers": [
      {
        "tag": "dns-proxy",
        "type": "tls",
        "server": "8.8.8.8",
        "server_port": 853,
        "tls": {},
        "detour": "out-proxy"
      },
      // {
      //   "tag": "dns-proxy",
      //   "type": "tls",
      //   "server": "1.1.1.1",
      //   "server_port": 853,
      //   "tls": {},
      //   "detour": "out-proxy"
      // },
      {
        "tag": "dns-local",
        "type": "tls",
        "server": "223.5.5.5",
        "server_port": 853,
        "tls": {},
        "detour": "direct"
      }// ,
      // {
      //   "tag": "dns-local",
      //   "type": "https",
      //   "server": "223.5.5.5",
      //   "server_port": 443,
      //   "path": "/dns-query",
      //   "tls": {},
      //   "detour": "direct"
      // },
      // {
      //   "tag": "dns-local",
      //   "type": "udp",
      //   "server": "223.5.5.5",
      //   "server_port": 53,
      //   "detour": "direct"
      // },
    ],
    // 默认使用第一个服务器
    // "final": "",
    "rules": [
      // 自定义服务器
      {
        "domain_suffix": [
          "shkits.com",
          "x-ha.com",
          "r-ci.com",
          "myip.biturl.top",
          "ifconfig.me",
          "ifconfig.io",
          "api.myip.la",
          "getip.icu",
          "ip.threep.top ip.sb",
          "test-ipv6.com"
        ],
        "action": "route",
        "server": "dns-local"
      },
      {
        "rule_set": "geosite-geolocation-cn",
        "action": "route",
        "server": "dns-local"
      },
      {
        "type": "logical",
        "mode": "and",
        "rules": [
          {
            "rule_set": "geosite-geolocation-!cn",
            "invert": true
          },
          {
            "rule_set": "geoip-cn"
          }
        ],
        "action": "route",
        // 代理DNS延长TTL，不然可能下游设备未过期这里就缓存失效了
        "rewrite_ttl": 600,
        "server": "dns-proxy"
      }
    ],
    // "cache_capacity": 65536,
    "reverse_mapping": true
  },
  "inbounds": [
    {
      "type": "socks",
      "tag": "in-socks",
      "listen": "::",
      "listen_port": 1080
    }, {
      "type": "http",
      "tag": "in-http",
      "listen": "::",
      "listen_port": 3128
    }, {
      "type": "trojan",
      "tag": "in-trojan",
      "listen": "::",
      "listen_port": 8372,
      "users": [{
        "name": "<USER NAME>",
        "password": "<PASSWORD>"
      }],
      "tls": {
        "enabled": true,
        "server_name": "example.org",
        "insecure": false,
        "alpn": ["h2"],
        "min_version": "1.2",
        "acme": {
          "domain": "example.org",
          "email": "admin@example.org",
          "data_directory": "/var/lib/vbox/certs",
          "provider": "zerossl", // letsencrypt, zerossl, https://...
          // ZeroSSL need external_account:
          //   + https://zerossl.com/documentation/api/generate-eab-credentials/
          //   + https://app.zerossl.com/developer
          "external_account": {
              "key_id": "<KEY ID>",
              "mac_key": "<MAC KEY>"
          },
          "dns01_challenge": {
            "provider": "cloudflare",
            "api_token": "my_token"
          }
        }
      },
      "fallback": {
        "server": "127.0.0.1",
        "server_port": 8375
      },
      "fallback_for_alpn": {
        "h2": {
          "server": "127.0.0.1",
          "server_port": 8375
        }
      }
    }
  ],
  "outbounds": [
    // 大陆白名单模式,第一个或final指向默认值
    {
      "type": "trojan",
      "tag": "out-proxy",
      "server": "<HOST>",
      "server_port": 8080,
      "password": "<PASSWORD>",
      "tls": {
        "enabled": true,
        "server_name": "example.org",
        "insecure": false,
        "alpn": ["h2"],
        "min_version": "1.2"
      } // ,
      // "multiplex": {
      //   "enabled": true
      // },
      // "domain_resolver": {
      //   "server": "dns-proxy-bootstrap" // ,
      //   // "rewrite_ttl": 60,
      //   // "strategy": "prefer_ipv4", // prefer_ipv4, prefer_ipv6, ipv4_only, ipv6_only
      //   // "client_subnet": "114.114.114.114/24" // Any China client IP address
      //   // "client_subnet": "202.96.209.133/24" // Shanghai ChinaNet IP address
      //   // "client_subnet": "210.22.70.3/24" // Shanghai ChinaUnion IP address
      // }
    },
    {
      "type": "direct",
      "tag": "direct",
      "domain_resolver": {
        "server": "dns-local" // ,
        // "rewrite_ttl": 60,
        // "strategy": "prefer_ipv4", // prefer_ipv4, prefer_ipv6, ipv4_only, ipv6_only
        // "client_subnet": "114.114.114.114/24" // Any China client IP address
        // "client_subnet": "202.96.209.133/24" // Shanghai ChinaNet IP address
        // "client_subnet": "210.22.70.3/24" // Shanghai ChinaUnion IP address
      }
    }
  ],
  "route": {
    "rules": [
      {
        "ip_is_private": true,
        "outbound": "direct"
      },
      {
        "rule_set": [
          "geosite-category-ads-all"
        ],
        "action": "reject"
      },
      {
        "protocol": [
          "bittorrent"
        ],
        "outbound": "direct"
      },
      {
        "port": [
          123,
          546
        ],
        "network": [
          "udp"
        ],
        "outbound": "direct"
      },
      {
        "source_port": [
           22,
           36000
        ],
        "network": [
          "tcp"
        ],
        "outbound": "direct"
      },
      {
        // Special domains
        "domain_suffix": [
          "shkits.com",
          "x-ha.com",
          "r-ci.com",
          "myip.biturl.top",
          "ifconfig.me",
          "ifconfig.io",
          "api.myip.la",
          "getip.icu",
          "ip.threep.top",
          "ip.sb",
          "test-ipv6.com",
          "letsencrypt.org",
          "zerossl.com"
        ],
        "outbound": "direct"
      }
    ],
    "rule_set": [
      {
        "type": "local",
        "tag": "geosite-geolocation-cn",
        "format": "binary",
        "path": "/usr/share/vbox/geosite/geosite-geolocation-cn.srs"
      },
      {
        "type": "local",
        "tag": "geosite-geolocation-!cn",
        "format": "binary",
        "path": "/usr/share/vbox/geosite/geosite-geolocation-!cn.srs"
      },
      {
        "type": "local",
        "tag": "geoip-cn",
        "format": "binary",
        "path": "/usr/share/vbox/geoip/geoip-cn.srs"
      },
      {
        "type": "local",
        "tag": "geosite-category-ads-all",
        "format": "binary",
        "path": "/usr/share/vbox/geosite/geosite-category-ads-all.srs"
      }
    ],
    // 默认出站标签。如果为空，将使用第一个可用于对应协议的出站。
    // "final": "",
    "auto_detect_interface": true,
    // For Android
    // "override_android_vpn": true,
    "default_mark": 511,
    "default_domain_resolver": {
      // 大陆白名单模式使用dns-proxy，否则默认用dns-local
      "server": "dns-proxy"
      // "rewrite_ttl": 60,
      // "client_subnet": "114.114.114.114/24" // Any China client IP address
      // "client_subnet": "202.96.209.133/24" // Shanghai ChinaNet IP address
      // "client_subnet": "210.22.70.3/24" // Shanghai ChinaUnion IP address
    }
  },
  "experimental": {
    "cache_file": {
      "enabled": true // ,
      // Available from 1.9.0
      // "store_rdrc": true
    }
  }
}
