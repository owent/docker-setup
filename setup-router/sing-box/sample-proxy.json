{
  "log": {
    "disabled": false,
    "level": "info",
    "output": "/var/log/vbox/box.log",
    "timestamp": true
  },
  "dns": {
    // When proxy server support ipv4 only
    "strategy": "prefer_ipv4", // prefer_ipv4,prefer_ipv6,ipv4_only,ipv6_only
    "servers": [
      {
        "tag": "dns-proxy",
        "address": "tls://8.8.8.8",
        "strategy": "ipv4_only"
      },
      // {
      //   "tag": "dns-proxy",
      //   "address": "tls://1.1.1.1"
      // },
      {
        "tag": "dns-local",
        "address": "tls://223.5.5.5",
        "detour": "direct"
      }// ,
      // {
      //   "tag": "dns-local",
      //   "address": "https://223.5.5.5/dns-query",
      //   "detour": "direct"
      // }
    ],
    // 默认使用第一个服务器
    // "final": "",
    "rules": [
      // 自定义服务器
      {
        "domain_suffix": [
          "shkits.com",
          "x-ha.com",
          "r-ci.com",
          "myip.biturl.top",
          "ifconfig.me",
          "ifconfig.io",
          "api.myip.la",
          "getip.icu",
          "ip.threep.top ip.sb",
          "test-ipv6.com"
        ],
        "server": "dns-local"
      },
      {
        "rule_set": "geosite-geolocation-cn",
        "server": "dns-local"
      },
      // client_subnet is available from 1.9.0
      // {
      //   "type": "logical",
      //   "mode": "and",
      //   "rules": [
      //     {
      //       "rule_set": "geosite-geolocation-!cn"
      //     },
      //     {
      //       "rule_set": "geoip-cn"
      //     }
      //   ],
      //   "server": "dns-proxy",
      //   "client_subnet": "114.114.114.114" // Any China client IP address
      // },
      {
        "outbound": "any",
        "server": "dns-proxy"
      }
    ]
  },
  "inbounds": [
    {
      "type": "trojan",
      "tag": "in-trojan",
      "listen": "::",
      "listen_port": 8372,
      "users": [{
        "name": "<USER NAME>",
        "password": "<PASSWORD>"
      }],
      "tls": {
        "enabled": true,
        "server_name": "example.org",
        "insecure": false,
        "alpn": ["h2"],
        "min_version": "1.2",
        "acme": {
          "domain": "example.org",
          "email": "admin@example.org",
          "data_directory": "/var/lib/vbox/certs",
          "provider": "zerossl", // letsencrypt, zerossl, https://...
          // ZeroSSL need external_account:
          //   + https://zerossl.com/documentation/api/generate-eab-credentials/
          //   + https://app.zerossl.com/developer
          "external_account": {
              "key_id": "xFHnEIOGIYYN5c6vzAqqog",
              "mac_key": ""
          },
          "dns01_challenge": {
            "provider": "cloudflare",
            "api_token": "my_token"
          }
        }
      },
      "fallback": {
        "server": "127.0.0.1",
        "server_port": 8375
      },
      "fallback_for_alpn": {
        "h2": {
          "server": "127.0.0.1",
          "server_port": 8375
        }
      }
    }
  ],
  "outbounds": [
    // 大陆白名单模式,第一个或final指向默认值
    {
      "type": "trojan",
      "tag": "out-proxy",
      "server": "<HOST>",
      "server_port": 8080,
      "password": "<PASSWORD>",
      "tls": {
        "enabled": true,
        "server_name": "example.org",
        "insecure": false,
        "alpn": ["h2"],
        "min_version": "1.2"
      } //,
      // "multiplex": {
      //   "enabled": true
      // }
    },
    {
      "type": "direct",
      "tag": "direct"
    },
    {
      "type": "dns",
      "tag": "dns-out"
    },
    {
      "type": "block",
      "tag": "block"
    }
  ],
  "route": {
    "rules": [
      {
        "ip_is_private": true,
        "outbound": "direct"
      },
      {
        "rule_set": [
          "geosite-category-ads-all"
        ],
        "outbound": "block"
      },
      {
        "protocol": [
          "bittorrent"
        ],
        "outbound": "direct"
      },
      {
        "port": [
          123,
          546
        ],
        "network": [
          "udp"
        ],
        "outbound": "direct"
      },
      {
        "source_port": [
           22,
           36000
        ],
        "network": [
          "tcp"
        ],
        "outbound": "direct"
      },
      {
        // Special domains
        "domain_suffix": [
          "shkits.com",
          "x-ha.com",
          "r-ci.com",
          "myip.biturl.top",
          "ifconfig.me",
          "ifconfig.io",
          "api.myip.la",
          "getip.icu",
          "ip.threep.top ip.sb",
          "test-ipv6.com",
          "letsencrypt.org",
          "zerossl.com"
        ],
        "outbound": "direct"
      },
      // When proxy server support ipv4 only
      {
        "ip_version": 6,
        "outbound": "direct"
      }
    ],
    "rule_set": [
      {
        "type": "local",
        "tag": "geosite-geolocation-cn",
        "format": "binary",
        "path": "/usr/share/vbox/geosite/geosite-geolocation-cn.srs"
      },
      {
        "type": "local",
        "tag": "geosite-geolocation-!cn",
        "format": "binary",
        "path": "/usr/share/vbox/geosite/geosite-geolocation-!cn.srs"
      },
      {
        "type": "local",
        "tag": "geoip-cn",
        "format": "binary",
        "path": "/usr/share/vbox/geoip/geoip-cn.srs"
      },
      {
        "type": "local",
        "tag": "geosite-category-ads-all",
        "format": "binary",
        "path": "/usr/share/vbox/geosite/geosite-category-ads-all.srs"
      }
    ],
    // 默认出站标签。如果为空，将使用第一个可用于对应协议的出站。
    // "final": "",
    "auto_detect_interface": true,
    // For Android
    // "override_android_vpn": true,
    "default_mark": 511
  },
  "experimental": {
    "cache_file": {
      "enabled": true // ,
      // Available from 1.9.0
      // "store_rdrc": true
    }
  }
}
