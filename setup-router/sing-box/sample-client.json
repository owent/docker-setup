{
  "log": {
    "disabled": false,
    "level": "error",
    // "output": "/var/log/vbox/box.log",
    "timestamp": true
  },
  "dns": {
    // When proxy server support ipv4 only
    // "strategy": "prefer_ipv4", // prefer_ipv4,prefer_ipv6,ipv4_only,ipv6_only
    "servers": [
      {
        "tag": "dns-proxy",
        "type": "tls",
        "server": "8.8.8.8",
        "server_port": 853,
        "tls": {},
        "detour": "out-proxy"
      },
      // {
      //   "tag": "dns-proxy",
      //   "type": "tls",
      //   "server": "1.1.1.1",
      //   "server_port": 853,
      //   "tls": {},
      //   "detour": "out-proxy"
      // },
      {
        "tag": "dns-local",
        "type": "tls",
        "server": "223.5.5.5",
        "server_port": 853,
        "tls": {},
        "detour": "direct"
      } // ,
      // {
      //   "tag": "dns-local",
      //   "type": "https",
      //   "server": "223.5.5.5",
      //   "server_port": 443,
      //   "path": "/dns-query",
      //   "tls": {},
      //   "detour": "direct"
      // }
      // ,
      // {
      //   "tag": "dns-local",
      //   "type": "local",,
      //   "detour": "direct"
      // }
    ],
    // 默认使用第一个服务器
    // "final": "",
    "rules": [
      // 自定义服务器
      {
        "domain_suffix": [
          "shkits.com",
          "x-ha.com",
          "r-ci.com",
          "myip.biturl.top",
          "ifconfig.me",
          "ifconfig.io",
          "api.myip.la",
          "getip.icu",
          "ip.threep.top",
          "ip.sb",
          "test-ipv6.com",
          "ipv6-test.com"
        ],
        "action": "route",
        "server": "dns-local"
      },
      {
        "rule_set": "geosite-geolocation-cn",
        "action": "route",
        "server": "dns-local"
      },
      {
        "source_ip_cidr": "172.22.0.0/16",
        "action": "route",
        "server": "dns-local"
      },
      {
        "type": "logical",
        "mode": "and",
        "rules": [
          {
            "rule_set": "geosite-geolocation-!cn",
            "invert": true
          },
          {
            "rule_set": "geoip-cn"
          }
        ],
        "action": "route",
        // 代理DNS延长TTL，不然可能下游设备未过期这里就缓存失效了
        "rewrite_ttl": 600,
        "server": "dns-proxy"
      }
    ],
    // "cache_capacity": 2097152,
    "reverse_mapping": true // ,
    // 如果上面路由规则里有配置fakeip,请确保fakeip的路由要转入 tun
    // "fakeip": {
    //   "enabled": true,
    //   "inet4_range": "172.28.0.1/14",
    //   "inet6_range": "fd02:0:1:ac1c::1/64"
    // }
  },
  "inbounds": [
    {
      "type": "tun",
      "tag": "in-tun",
      "interface_name": "tun-vbox",
      "address": ["172.19.0.1/30", "fd08:0:0:ac13::1/126"],
      "mtu": 9000,
      "auto_route": true,
      "iproute2_table_index": 2022,
      "iproute2_rule_index": 9000,
      "auto_redirect": false,
      // Default: 0x2023
      "auto_redirect_input_mark": "13",
      // Default: 0x2024
      "auto_redirect_output_mark": "15",
      "strict_route": true,
      "stack": "mixed",
      // "endpoint_independent_nat": false,
      // 开启ROUTER_NET_LOCAL_ENABLE_VBOX时尽量关闭tun的include和excelude规则，性能很差
      // include_interface": ["enp2s0"],
      "exclude_interface": ["vlan.enp2s0"],
      "route_exclude_address": [
        "0.0.0.0/8",
        "10.0.0.0/8",
        "127.0.0.0/8",
        "169.254.0.0/16",
        "172.16.0.0/12",
        "192.0.0.0/24",
        "192.168.0.0/16",
        "224.0.0.0/4",
        "240.0.0.0/4",
        "::1/128",
        "::/128",
        "::ffff:0:0/96",
        "64:ff9b::/96",
        "100::/64",
        "fc00::/7",
        "fe80::/10",
        "ff00::/8"
        // ROUTE_EXLUCDE_ADDRESS_PLACEHOLDER
      ],
      // "route_address_set": [],
      // "route_exclude_address_set": ["geoip-cn"],
      "udp_timeout": "5m"
    },
    {
      "type": "trojan",
      "tag": "in-trojan",
      "listen": "::",
      "listen_port": 8372,
      "users": [
        {
          "name": "<USER NAME>",
          "password": "<PASSWORD>"
        }
      ],
      "tls": {
        "enabled": true,
        "server_name": "example.org",
        "insecure": false,
        "alpn": ["h2"],
        "min_version": "1.2",
        "certificate_path": "/data/ssl/<CERT OR FULLCHAIN>",
        "key_path": "/data/ssl/<KEY>"
      },
      "fallback": {
        "server": "127.0.0.1",
        "server_port": 8375
      },
      "fallback_for_alpn": {
        "h2": {
          "server": "127.0.0.1",
          "server_port": 8375
        }
      }
    },
    {
      "type": "http",
      "tag": "in-http",

      "listen": "127.0.0.1",
      "listen_port": 3128,
      // "users": [
      //   {
      //     "username": "owent",
      //     "password": "admin"
      //   }
      // ],
      // "tls": {},
      "set_system_proxy": false
    },
    {
      "type": "socks",
      "tag": "in-socks",

      "listen": "127.0.0.1",
      "listen_port": 1080
      // "users": [
      //   {
      //     "username": "owent",
      //     "password": "admin"
      //   }
      // ],
    }
  ],
  "outbounds": [
    // 大陆白名单模式,第一个或final指向默认值
    {
      "type": "trojan",
      "tag": "out-proxy",
      "server": "<HOST>",
      "server_port": 8080,
      "password": "<PASSWORD>",
      "tls": {
        "enabled": true,
        "server_name": "example.org",
        "insecure": false,
        "alpn": ["h2"],
        "min_version": "1.2"
      },
      // "multiplex": {
      //   "enabled": true
      // },
      // "domain_resolver": {
      //   "server": "dns-proxy-bootstrap" // ,
      //   // "rewrite_ttl": 60,
      //   // "strategy": "prefer_ipv4", // prefer_ipv4, prefer_ipv6, ipv4_only, ipv6_only
      //   // "client_subnet": "114.114.114.114/24" // Any China client IP address
      //   // "client_subnet": "202.96.209.133/24" // Shanghai ChinaNet IP address
      //   // "client_subnet": "210.22.70.3/24" // Shanghai ChinaUnion IP address
      // }
    },
    {
      "type": "direct",
      "tag": "direct",
      "domain_resolver": {
        "server": "dns-local" // ,
        // "rewrite_ttl": 60,
        // "strategy": "prefer_ipv4", // prefer_ipv4, prefer_ipv6, ipv4_only, ipv6_only
        // "client_subnet": "114.114.114.114/24" // Any China client IP address
        // "client_subnet": "202.96.209.133/24" // Shanghai ChinaNet IP address
        // "client_subnet": "210.22.70.3/24" // Shanghai ChinaUnion IP address
      }
    }
  ],
  "route": {
    "rules": [
      {
        "inbound": "in-tun",
        "action": "sniff",
        // "sniffer": ["http", "tls", "quic", "dns", "bittorrent", "dtls", "rdp"],
        "timeout": "3s"
      },
      {
        "protocol": "dns",
        "action": "hijack-dns"
      },
      {
        "source_ip_cidr": "172.22.0.0/16",
        "action": "route",
        "outbound": "direct"
      },
      {
        "ip_is_private": true,
        "action": "route",
        "outbound": "direct"
      },
      {
        "rule_set": ["geosite-category-ads-all"],
        "action": "reject"
      },
      {
        "protocol": ["bittorrent"],
        "action": "route",
        "outbound": "direct"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "port": 853
          },
          {
            "network": "udp",
            "port": 443
          },
          {
            "protocol": "stun"
          }
        ],
        "action": "reject"
      },
      {
        "port": [67, 68, 123, 546, 547],
        "network": ["udp"],
        "action": "route",
        "outbound": "direct"
      },
      {
        "source_port": [22, 36000],
        "network": ["tcp"],
        "action": "route",
        "outbound": "direct"
      },
      {
        "source_port": [6096, 6349, 6443, 6881, 6882, 6883, 8371, 8372, 8373],
        "action": "route",
        "outbound": "direct"
      },
      {
        "rule_set": ["geoip-cn", "geosite-geolocation-cn", "geosite-speedtest"],
        "action": "route",
        "outbound": "direct"
      },
      {
        // Special domains
        "domain_suffix": [
          "shkits.com",
          "x-ha.com",
          "r-ci.com",
          "myip.biturl.top",
          "ifconfig.me",
          "ifconfig.io",
          "api.myip.la",
          "getip.icu",
          "ip.threep.top",
          "ip.sb",
          "test-ipv6.com",
          "ipv6-test.com"
        ],
        "action": "route",
        "outbound": "direct"
      }
    ],
    "rule_set": [
      {
        "type": "local",
        "tag": "geosite-geolocation-cn",
        "format": "binary",
        "path": "/usr/share/vbox/geosite/geosite-geolocation-cn.srs"
      },
      {
        "type": "local",
        "tag": "geosite-geolocation-!cn",
        "format": "binary",
        "path": "/usr/share/vbox/geosite/geosite-geolocation-!cn.srs"
      },
      {
        "type": "local",
        "tag": "geoip-cn",
        "format": "binary",
        "path": "/usr/share/vbox/geoip/geoip-cn.srs"
      },
      {
        "type": "local",
        "tag": "geosite-speedtest",
        "format": "binary",
        "path": "/usr/share/vbox/geosite/geosite-speedtest.srs"
      },
      {
        "type": "local",
        "tag": "geosite-category-ads-all",
        "format": "binary",
        "path": "/usr/share/vbox/geosite/geosite-category-ads-all.srs"
      },
      {
        // 非大陆白名单模式请代理这个
        "type": "local",
        "tag": "geosite-connectivity-check",
        "format": "binary",
        "path": "/usr/share/vbox/geosite/geosite-connectivity-check.srs"
      }
    ],
    // 默认出站标签。如果为空，将使用第一个可用于对应协议的出站。
    // "final": "",
    // 有时候sing-box的自动检测会有冲突，最好手动指定
    "auto_detect_interface": false,
    // "default_interface": "ppp0",
    // For Android
    // "override_android_vpn": true,
    "default_mark": 15,
    "default_domain_resolver": {
      // 大陆白名单模式使用dns-proxy，否则默认用dns-local
      "server": "dns-proxy",
      // "rewrite_ttl": 60,
      // "client_subnet": "114.114.114.114/24" // Any China client IP address
      // "client_subnet": "202.96.209.133/24" // Shanghai ChinaNet IP address
      "client_subnet": "210.22.70.3/24" // Shanghai ChinaUnion IP address
    }
  },
  "experimental": {
    "cache_file": {
      "enabled": true // ,
      // Available from 1.9.0
      // "store_rdrc": true
    },
    "clash_api": {
      "default_mode": "Enhanced"
    }
  }
}
