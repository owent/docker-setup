  "route": {
    "rules": [
      {
        "inbound": "in-tun",
        "action": "sniff",
        // 不sniff SSH协议，似乎会导致DNS解析不稳定
        "sniffer": ["http", "tls", "quic", "stun", "dns", "bittorrent", "dtls", "rdp", "ntp"],
        "timeout": "3s"
      },
      {
        "protocol": "dns",
        "action": "hijack-dns"
      },
      {
        "source_ip_cidr": "172.22.0.0/16",
        "action": "route",
        "outbound": "direct"
      },
      {
        "ip_is_private": true,
        "action": "route",
        "outbound": "direct"
      },
      // DNS Servers
      {
        "ip_cidr": [
           "8.8.8.8/32",
           "8.8.4.4/32",
           "2001:4860:4860::8888/128",
           "2001:4860:4860::8888/128",
           // "1.1.1.1/32",
           // "1.0.0.1/32",
           // "2606:4700:4700::1111/128",
           // "2606:4700:4700::1001/128",
           "9.9.9.0/24",
           "149.112.112.0/24",
           "2620:fe::/112",
           "2620:fe::fe:00/112"
        ],
        "action": "route",
        "outbound": "out-proxy"
      },
      {
        "rule_set": ["geosite-category-ads-all"],
        "action": "reject"
      },
      {
        "protocol": ["bittorrent"],
        "action": "route",
        "outbound": "direct"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "port": 853
          },
          {
            "network": "udp",
            "port": 443
          },
          {
            "protocol": "stun"
          }
        ],
        "action": "reject"
      },
      {
        "port": [67, 68, 123, 546, 547],
        "network": ["udp"],
        "action": "route",
        "outbound": "direct"
      },
      {
        "source_port": [22, 36000],
        "network": ["tcp"],
        "action": "route",
        "outbound": "direct"
      },
      {
        "source_port": [6096, 6349, 6443, 6881, 6882, 6883, 8371, 8372, 8373],
        "action": "route",
        "outbound": "direct"
      },
      {
        "rule_set": ["geoip-cn", "geosite-geolocation-cn", "geosite-speedtest"],
        "action": "route",
        "outbound": "direct"
      },
      {
        // Special domains
        "domain_suffix": [
          "shkits.com",
          "x-ha.com",
          "r-ci.com",
          "myip.biturl.top",
          "ifconfig.me",
          "ifconfig.io",
          "api.myip.la",
          "getip.icu",
          "ip.threep.top",
          "ip.sb",
          "test-ipv6.com",
          "ipv6-test.com"
        ],
        "action": "route",
        "outbound": "direct"
      }
    ],
    "rule_set": [
      {
        "type": "local",
        "tag": "geosite-geolocation-cn",
        "format": "binary",
        "path": "/usr/share/vbox/geosite/geosite-geolocation-cn.srs"
      },
      {
        "type": "local",
        "tag": "geosite-geolocation-!cn",
        "format": "binary",
        "path": "/usr/share/vbox/geosite/geosite-geolocation-!cn.srs"
      },
      {
        "type": "local",
        "tag": "geoip-cn",
        "format": "binary",
        "path": "/usr/share/vbox/geoip/geoip-cn.srs"
      },
      {
        "type": "local",
        "tag": "geosite-speedtest",
        "format": "binary",
        "path": "/usr/share/vbox/geosite/geosite-speedtest.srs"
      },
      {
        "type": "local",
        "tag": "geosite-category-ads-all",
        "format": "binary",
        "path": "/usr/share/vbox/geosite/geosite-category-ads-all.srs"
      },
      {
        // 非大陆白名单模式请代理这个
        "type": "local",
        "tag": "geosite-connectivity-check",
        "format": "binary",
        "path": "/usr/share/vbox/geosite/geosite-connectivity-check.srs"
      }
    ],
    // 默认出站标签。如果为空，将使用第一个可用于对应协议的出站。
    // "final": "",
    // 有时候sing-box的自动检测会有冲突，最好手动指定
    "auto_detect_interface": false,
    // "default_interface": "ppp0",
    // For Android
    // "override_android_vpn": true,
    "default_mark": 15,
    "default_domain_resolver": {
      // 大陆白名单模式使用dns-proxy，否则默认用dns-local
      "server": "dns-proxy"
      // "rewrite_ttl": 60,
    }
  },
  "experimental": {
    "cache_file": {
      "enabled": true // ,
      // Available from 1.9.0
      // "store_rdrc": true
    },
    "clash_api": {
      "default_mode": "Enhanced"
    }
