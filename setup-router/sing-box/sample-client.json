      "type": "trojan",
      "tag": "out-proxy",
      "server": "<HOST>",
      "server_port": 8080,
      "password": "<PASSWORD>",
      "tls": {
        "enabled": true,
        "server_name": "example.org",
        "insecure": false,
        "alpn": ["h2", "http/1.1"],
