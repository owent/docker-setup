{
  "log": {
    "disabled": false,
    // trace debug info warn error fatal panic
    "level": "error",
    // "output": "/var/log/vbox/box.log",
    "timestamp": true
  },
  "dns": {
    // When proxy server support ipv4 only
    // "strategy": "prefer_ipv4", // prefer_ipv4,prefer_ipv6,ipv4_only,ipv6_only
    "servers": [
      {
        "tag": "dns-proxy",
        "type": "tls",
        "server": "8.8.8.8",
        "server_port": 853,
        "tls": {},
        "detour": "out-proxy-dns"
      },
      // {
      //   "tag": "dns-proxy",
      //   "type": "tls",
      //   "server": "1.1.1.1",
      //   "server_port": 853,
      //   "tls": {},
      //   "detour": "out-proxy-dns"
      // },
      {
        "tag": "dns-local",
        "type": "tls",
        "server": "223.5.5.5",
        "server_port": 853,
        "tls": {},
        "detour": "direct"
      } // ,
      // {
      //   "tag": "dns-local",
      //   "type": "https",
      //   "server": "223.5.5.5",
      //   "server_port": 443,
      //   "path": "/dns-query",
      //   "tls": {},
      //   "detour": "direct"
      // },
      // {
      //   "tag": "dns-local",
      //   "type": "udp",
      //   "server": "223.5.5.5",
      //   "server_port": 53,
      //   "detour": "direct"
      // },
      // {
      //   "tag": "dns-local",
      //   "type": "local",
      //   "detour": "direct"
      // }
    ],
    // 默认使用第一个服务器
    // "final": "",
    "rules": [
      // 自定义服务器
      {
        "domain_suffix": [
          "shkits.com",
          "x-ha.com",
          "r-ci.com",
          "myip.biturl.top",
          "ifconfig.me",
          "ifconfig.io",
          "api.myip.la",
          "getip.icu",
          "ip.threep.top",
          "ip.sb",
          "test-ipv6.com",
          "ipv6-test.com"
        ],
        "action": "route",
        "server": "dns-local"
      },
      {
        "rule_set": "geosite-geolocation-cn",
        "action": "route",
        "server": "dns-local"
      },
      {
        "source_ip_cidr": "172.22.0.0/16",
        "action": "route",
        "server": "dns-local"
      },
      {
        "type": "logical",
        "mode": "and",
        "rules": [
          {
            "rule_set": "geosite-geolocation-!cn",
            "invert": true
          },
          {
            // 走“乐观解析”（Optimistic Resolution）
            "rule_set": "geoip-cn"
          }
        ],
        "action": "route",
        // 代理DNS延长TTL，不然可能下游设备未过期这里就缓存失效了
        "rewrite_ttl": 600,
        "server": "dns-local"
      }
      // , {
      //   "action": "route",
      //   "server": "dns-proxy",
      //   // 代理DNS延长TTL，不然可能下游设备未过期这里就缓存失效了
      //   "rewrite_ttl": 600
