      //   // "client_subnet": "202.96.209.133/24" // Shanghai ChinaNet IP address
      //   // "client_subnet": "210.22.70.3/24" // Shanghai ChinaUnion IP address
      // }
    ],
    // "cache_capacity": 2097152,
    "reverse_mapping": true // ,
    // 如果上面路由规则里有配置fakeip,请确保fakeip的路由要转入 tun
    // "fakeip": {
    //   "enabled": true,
    //   "inet4_range": "172.28.0.1/14",
    //   "inet6_range": "fd02:0:1:ac1c::1/64"
    // }
  },
  "inbounds": [
    {
      "type": "tun",
      "tag": "in-tun",
      "interface_name": "tun-vbox",
      "address": ["172.19.0.1/30", "fd08:0:0:ac13::1/126"],
      // "mtu": 9000,
      "mtu": 1380,
      "auto_route": true,
      "iproute2_table_index": 2022,
      "iproute2_rule_index": 9000,
      "auto_redirect": false,
      // Default: 0x2023
      "auto_redirect_input_mark": "3",
      // Default: 0x2024
      "auto_redirect_output_mark": "15",
      "strict_route": true,
      "stack": "mixed",
      // "stack": "gvisor",
      // 1.12.0版本测试，gvisor模式比mixed模式
      // - download开销大约高50% -> 400Mbps: CPU 60% vs 90%
      // - upload开销低60% -> 400Mbps: CPU 50% vs 20%
      // "endpoint_independent_nat": false,
      // 开启ROUTER_NET_LOCAL_ENABLE_VBOX时尽量关闭tun的include和excelude规则，性能很差
      // include_interface": ["enp2s0"],
      "exclude_interface": ["vlan.enp2s0"],
      "route_exclude_address": [
        "0.0.0.0/8",
        "10.0.0.0/8",
        "127.0.0.0/8",
        "169.254.0.0/16",
        "172.16.0.0/12",
        "192.0.0.0/24",
        "192.168.0.0/16",
        "224.0.0.0/4",
        "240.0.0.0/4",
        "::1/128",
        "::/128",
        "::ffff:0:0/96",
        "64:ff9b::/96",
        "100::/64",
        "fc00::/7",
        "fe80::/10",
        "ff00::/8"
        // ROUTE_EXLUCDE_ADDRESS_PLACEHOLDER
      ],
      // "route_address_set": [],
      // "route_exclude_address_set": ["geoip-cn"],
      // 建议值 < /proc/sys/net/netfilter/nf_conntrack_udp_timeout_stream - /proc/sys/net/netfilter/nf_conntrack_udp_timeout
      //   Linux 默认值一般是 120s - 30s = 90s
      //   建议改大 /proc/sys/net/netfilter/nf_conntrack_udp_timeout_stream 到 600s。udp_timeout保持5m。
      "udp_timeout": "5m"
    },
    {
      "type": "trojan",
      "tag": "in-trojan",
      "listen": "::",
      "listen_port": 8372,
      "tcp_fast_open": true,
      "tcp_multi_path": true,
      "users": [
        {
          "name": "<USER NAME>",
          "password": "<PASSWORD>"
        }
      ],
      "tls": {
        "enabled": true,
        "server_name": "example.org",
        "insecure": false,
        "alpn": ["h2"],
        "min_version": "1.3",
        "certificate_path": "/data/ssl/<CERT OR FULLCHAIN>",
        "key_path": "/data/ssl/<KEY>"
      },
      "fallback": {
        "server": "127.0.0.1",
        "server_port": 8375
      },
      "fallback_for_alpn": {
        "h2": {
          "server": "127.0.0.1",
          "server_port": 8375
        }
      }
    },
    {
      "type": "http",
      "tag": "in-http",

      "listen": "127.0.0.1",
      "listen_port": 3128,
      // "users": [
      //   {
      //     "username": "owent",
      //     "password": "admin"
      //   }
      // ],
      // "tls": {},
      "set_system_proxy": false
    },
    {
      "type": "socks",
      "tag": "in-socks",

      "listen": "127.0.0.1",
      "listen_port": 1080
      // "users": [
      //   {
      //     "username": "owent",
      //     "password": "admin"
      //   }
      // ],
    }
  ],
