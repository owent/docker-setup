      }
    },
    {
      "type": "vless",
      "tag": "out-proxy-reality",
      "server": "<HOST>",
      "server_port": 443,
      "uuid": "<UUID>",
      "flow": "xtls-rprx-vision",
      "packet_encoding": "xudp",
      "tls": {
        "enabled": true,
        "server_name": "www.microsoft.com", // 要和server端的 tls.reality.handshake.server 保持一致
        "insecure": false,
        "alpn": ["h2", "http/1.1"],
        "min_version": "1.3", // reality 需求 1.3 以上
        "utls": {
          "enabled": true,
          "fingerprint": "chrome" // 实测 randomized 在开启 reality 时兼容性不好
        },
        "reality": {
          "enabled": true,
          "public_key": "Generated by: vbox generate reality-keypair",
          "short_id": "Generated by: vbox generate rand --hex 8"
