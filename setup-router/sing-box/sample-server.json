{
  "log": {
    "disabled": false,
    "level": "info",
    // "output": "/var/log/vbox/box.log",
    "timestamp": true
  },
  "dns": {
    // When proxy server support ipv4 only
    // "strategy": "prefer_ipv4", // prefer_ipv4,prefer_ipv6,ipv4_only,ipv6_only
    "servers": [
      {
        "tag": "dns-proxy",
        "type": "tls",
        "server": "8.8.8.8",
        "server_port": 853,
        "tls": {},
        "detour": "out-proxy"
      },
      // {
      //   "tag": "dns-proxy",
      //   "type": "tls",
      //   "server": "1.1.1.1",
      //   "server_port": 853,
      //   "tls": {},
      //   "detour": "out-proxy"
      // },
      {
        "tag": "dns-local",
        "type": "tls",
        "server": "223.5.5.5",
        "server_port": 853,
        "tls": {},
        "detour": "direct"
      } // ,
      // {
      //   "tag": "dns-local",
      //   "type": "https",
      //   "server": "223.5.5.5",
      //   "server_port": 443,
      //   "path": "/dns-query",
      //   "tls": {},
      //   "detour": "direct"
      // }
    ],
    // 默认使用第一个服务器
    // "final": "",
    "rules": [
      // 自定义服务器
      {
        "domain_suffix": [
          "shkits.com",
          "x-ha.com",
          "r-ci.com",
          "myip.biturl.top",
          "ifconfig.me",
          "ifconfig.io",
          "api.myip.la",
          "getip.icu",
          "ip.threep.top ip.sb",
          "test-ipv6.com"
        ],
        "action": "route",
        "server": "dns-local"
      },
      {
        "rule_set": "geosite-geolocation-cn",
        "action": "route",
        "server": "dns-local"
      },
      {
        "type": "logical",
        "mode": "and",
        "rules": [
          {
            "rule_set": "geosite-geolocation-!cn",
            "invert": true
          },
          {
            "rule_set": "geoip-cn"
          }
        ],
        "action": "route",
        // 代理DNS延长TTL，不然可能下游设备未过期这里就缓存失效了
        "rewrite_ttl": 600,
        "server": "dns-proxy"
      }
    ]
  },
  "inbounds": [
    {
      "type": "vmess",
      "tag": "in-vmess",
      "listen": "::",
      "listen_port": 8371,
      "tcp_fast_open": false,
      "proxy_protocol": false,
      "users": [
        {
          "name": "<USER NAME>",
          "uuid": "<UUID>",
          "alterId": 256
        }
      ],
      "tls": {
        "enabled": true,
        "server_name": "example.org",
        "insecure": false,
        "alpn": ["h2"],
        "min_version": "1.2",
        "certificate_path": "<CERT/FULLCHAIN FILE PATH>",
        "key_path": "<KEY FILE PATH>"
      },
      "transport": {
        "type": "ws",
        "path": "/redirect",
        "max_early_data": 2048,
        "early_data_header_name": "Sec-WebSocket-Protocol"
      }
    },
    {
      "type": "trojan",
      "tag": "in-trojan",
      "listen": "::",
      "listen_port": 8372,
      "users": [
        {
          "name": "<USER NAME>",
          "password": "<PASSWORD>"
        }
      ],
      "tls": {
        "enabled": true,
        "server_name": "example.org",
        "insecure": false,
        "alpn": ["h2"],
        "min_version": "1.2",
        "certificate_path": "<CERT/FULLCHAIN FILE PATH>",
        "key_path": "<KEY FILE PATH>"
      },
      "fallback": {
        "server": "127.0.0.1",
        "server_port": 8375
      },
      "fallback_for_alpn": {
        "h2": {
          "server": "127.0.0.1",
          "server_port": 8375
        }
      }
    },
    {
      "type": "vless",
      "tag": "in-vless",
      "listen": "::",
      "listen_port": 8373,
      "tcp_fast_open": false,
      "proxy_protocol": false,
      "users": [
        {
          "name": "<USER NAME>",
          "uuid": "<UUID>",
          "flow": ""
        }
      ],
      "tls": {
        "enabled": true,
        "server_name": "example.org",
        "insecure": false,
        "alpn": ["h2", "http/1.1"],
        "min_version": "1.2",
        "certificate_path": "<CERT/FULLCHAIN FILE PATH>",
        "key_path": "<KEY FILE PATH>"
      },
      "transport": {
        "type": "ws",
        "path": "/redirect",
        "max_early_data": 2048,
        "early_data_header_name": "Sec-WebSocket-Protocol"
      }
    }
  ],
  "outbounds": [
    {
      "type": "direct",
      "tag": "direct",
      "domain_resolver": {
        "server": "dns-local" // ,
        // "rewrite_ttl": 60,
        // "strategy": "prefer_ipv4", // prefer_ipv4, prefer_ipv6, ipv4_only, ipv6_only
        // "client_subnet": "114.114.114.114/24" // Any China client IP address
        // "client_subnet": "202.96.209.133/24" // Shanghai ChinaNet IP address
        // "client_subnet": "210.22.70.3/24" // Shanghai ChinaUnion IP address
      }
    }
  ],
  "route": {
    "rules": [
      {
        "inbound": ["in-vmess", "in-vless"],
        "action": "sniff",
        // "sniffer": ["http", "tls", "quic", "dns", "bittorrent", "dtls", "rdp"],
        "timeout": "1s"
      },
      {
        "protocol": "dns",
        "action": "hijack-dns"
      }
    ],
    "rule_set": [
      {
        "type": "local",
        "tag": "geosite-geolocation-cn",
        "format": "binary",
        "path": "/usr/share/vbox/geosite/geosite-geolocation-cn.srs"
      },
      {
        "type": "local",
        "tag": "geosite-geolocation-!cn",
        "format": "binary",
        "path": "/usr/share/vbox/geosite/geosite-geolocation-!cn.srs"
      },
      {
        "type": "local",
        "tag": "geoip-cn",
        "format": "binary",
        "path": "/usr/share/vbox/geoip/geoip-cn.srs"
      },
      {
        "type": "local",
        "tag": "geosite-category-ads-all",
        "format": "binary",
        "path": "/usr/share/vbox/geosite/geosite-category-ads-all.srs"
      }
    ],
    // 默认出站标签。如果为空，将使用第一个可用于对应协议的出站。
    "final": "direct",
    "default_domain_resolver": {
      "server": "dns-local",
      // "rewrite_ttl": 60,
      // "client_subnet": "114.114.114.114/24" // Any China client IP address
      // "client_subnet": "202.96.209.133/24" // Shanghai ChinaNet IP address
      "client_subnet": "210.22.70.3/24" // Shanghai ChinaUnion IP address
    }
  },
  "experimental": {
    "cache_file": {
      "enabled": true // ,
      // Available from 1.9.0
      // "store_rdrc": true
    }
  }
}
