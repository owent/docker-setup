{
  "log": {
    "disabled": false,
    // trace debug info warn error fatal panic
    "level": "error",
    // "output": "/var/log/vbox/box.log",
    "timestamp": true
  },
  "dns": {
    // When proxy server support ipv4 only
    // "strategy": "prefer_ipv4", // prefer_ipv4,prefer_ipv6,ipv4_only,ipv6_only
    "servers": [
      {
        "tag": "dns-proxy",
        "type": "tls",
        "server": "8.8.8.8",
        "server_port": 853,
        "tls": {},
        "detour": "direct"
      },
      // {
      //   "tag": "dns-proxy",
      //   "type": "tls",
      //   "server": "1.1.1.1",
      //   "server_port": 853,
      //   "tls": {},
      //   "detour": "direct"
      // },
      {
        "tag": "dns-local",
        "type": "tls",
        "server": "223.5.5.5",
        "server_port": 853,
        "tls": {},
        "detour": "direct"
      } // ,
      // {
      //   "tag": "dns-local",
      //   "type": "https",
      //   "server": "223.5.5.5",
      //   "server_port": 443,
      //   "path": "/dns-query",
      //   "tls": {},
      //   "detour": "direct"
      // },
      // {
      //   "tag": "dns-local",
      //   "type": "udp",
      //   "server": "223.5.5.5",
      //   "server_port": 53,
      //   "detour": "direct"
      // },
    ],
    // 默认使用第一个服务器
    // "final": "",
    "rules": [
      // 自定义服务器
      {
        "domain_suffix": [
          "shkits.com",
          "x-ha.com",
          "r-ci.com",
          "myip.biturl.top",
          "ifconfig.me",
          "ifconfig.io",
          "api.myip.la",
          "getip.icu",
          "ip.threep.top",
          "ip.sb",
          "test-ipv6.com"
        ],
        "action": "route",
        "server": "dns-local"
      },
      {
        "rule_set": "geosite-geolocation-cn",
        "action": "route",
        "server": "dns-local"
      },
      {
        "type": "logical",
        "mode": "and",
        "rules": [
          {
            "rule_set": "geosite-geolocation-!cn",
            "invert": true
          },
          {
            "rule_set": "geoip-cn"
          }
        ],
        "action": "route",
        // 代理DNS延长TTL，不然可能下游设备未过期这里就缓存失效了
        "rewrite_ttl": 600,
        "server": "dns-proxy"
      }
    ]
  },
  "inbounds": [
    {
      "type": "vless",
      "tag": "in-vless-reality",
      "listen": "::",
      "listen_port": 443,
      "tcp_fast_open": true,
      "tcp_multi_path": true,
      "users": [
        {
          "name": "<USER NAME>",
          "uuid": "<UUID>",
          "flow": "xtls-rprx-vision"
        }
      ],
      "tls": {
        "enabled": true,
        "server_name": "cdn.jsdelivr.net", // 启用reality则必须和 tls.reality.handshake.server 一致
        "insecure": false,
        "alpn": ["h2", "http/1.1"],
        "min_version": "1.3", // xtls-rprx-vision 需求 1.3 以上
        "reality": {
          "enabled": true,
          "handshake": {
            "server": "cdn.jsdelivr.net",
            // 兼容性较好的域名: (推荐使用 RealiTLScanner 或 Sing-box 自带的测试命令扫描，延迟 <= 20ms|50ms)
            // - www.microsoft.com , itunes.apple.com , azure.microsoft.com , vscode.dev , update.microsoft.com
            // - www.amazon.com , aws.amazon.com , www.digitalocean.com , www.oracle.com , dw
            // - addons.mozilla.org , swdist.apple.com , dl.google.com
            "server_port": 443
          },
          "private_key": "Generated by: vbox generate reality-keypair",
          "short_id": [
            "Generated by: vbox generate rand --hex 8"
          ],
          "max_time_difference": "5m"
        }
      },
      // xtls-rprx-vision 和多路复用冲突，不能使用
      "multiplex": {
        "enabled": false
      }
    },
    {
      "type": "vmess",
      "tag": "in-vmess",
      "listen": "::",
      "listen_port": 8371,
      "tcp_fast_open": true,
      "tcp_multi_path": true,
      "users": [
        {
          "name": "<USER NAME>",
          "uuid": "<UUID>",
          "alterId": 256
        }
      ],
      "tls": {
        "enabled": true,
        "server_name": "example.org",
        "insecure": false,
        "alpn": ["h2", "http/1.1"],
        "min_version": "1.3",
        "certificate_path": "<CERT/FULLCHAIN FILE PATH>",
        "key_path": "<KEY FILE PATH>"
      },
      "transport": {
        "type": "ws",
        "path": "/redirect",
        "max_early_data": 2048,
        "early_data_header_name": "Sec-WebSocket-Protocol"
      }
    },
    {
      "type": "trojan",
      "tag": "in-trojan",
      "listen": "::",
      "listen_port": 8372,
      "tcp_fast_open": true,
      "tcp_multi_path": true,
      "users": [
        {
          "name": "<USER NAME>",
          "password": "<PASSWORD>"
        }
      ],
      "tls": {
        "enabled": true,
        "server_name": "example.org",
        "insecure": false,
        "alpn": ["h2", "http/1.1"],
        "min_version": "1.3",
        "certificate_path": "<CERT/FULLCHAIN FILE PATH>",
        "key_path": "<KEY FILE PATH>"
      },
      "fallback": {
        "server": "127.0.0.1",
        "server_port": 8375
      },
      "fallback_for_alpn": {
        "h2": {
          "server": "127.0.0.1",
          "server_port": 8375
        }
      }
    },
    {
      "type": "vless",
      "tag": "in-vless",
      "listen": "::",
      "listen_port": 8373,
      "tcp_fast_open": true,
      "tcp_multi_path": true,
      "users": [
        {
          "name": "<USER NAME>",
          "uuid": "<UUID>",
          "flow": "xtls-rprx-vision" // 这个功能要求 transport.type 为 tcp （默认）
        }
      ],
      "tls": {
        "enabled": true,
        "server_name": "example.org",
        "insecure": false,
        "alpn": ["h2", "http/1.1"],
        "min_version": "1.3",
        "certificate_path": "<CERT/FULLCHAIN FILE PATH>",
        "key_path": "<KEY FILE PATH>"
      },
      // xtls-rprx-vision 要求 transport.type 为 tcp （默认）
      // "transport": {
      //   "type": "ws",
      //   "path": "/redirect",
      //   "max_early_data": 2048,
      //   "early_data_header_name": "Sec-WebSocket-Protocol"
      // }
      // xtls-rprx-vision 和多路复用冲突，不能使用
      "multiplex": {
        "enabled": false
      }
    },
    {
      "type": "hysteria2",
      "tag": "in-hy2",
      "listen": "::",
      "listen_port": 8374,
      // "up_mbps": 100,
      // "down_mbps": 100,
      "obfs": {
        "type": "salamander",
        "password": "<PASSWORD>"
      },
      "users": [
        {
          "name": "<Authentication user>",
          "password": "<Authentication password>"
        }
      ],
      // "ignore_client_bandwidth": false,
      "tls": {
        "enabled": true,
        "server_name": "example.org",
        "key_path": "/path/to/key.pem",
        "certificate_path": "/path/to/certificate.pem"
        // "min_version": "1.3",
      },
      "masquerade": "http://127.0.0.1:8375", // file,http or https
      "brutal_debug": false
    }
  ],
  "outbounds": [
    {
      "type": "direct",
      "tag": "direct",
      "domain_resolver": {
        "server": "dns-local" // ,
        // "rewrite_ttl": 60,
        // "strategy": "prefer_ipv4", // prefer_ipv4, prefer_ipv6, ipv4_only, ipv6_only
        // "client_subnet": "114.114.114.114/24" // Any China client IP address
        // "client_subnet": "202.96.209.133/24" // Shanghai ChinaNet IP address
        // "client_subnet": "210.22.70.3/24" // Shanghai ChinaUnion IP address
      }
    }
  ],
  "route": {
    "rules": [
      {
        "action": "sniff",
        // 不sniff SSH协议，似乎会导致DNS解析不稳定
        "sniffer": ["http", "tls", "quic", "stun", "dns", "bittorrent", "dtls", "rdp", "ntp"],
        "timeout": "1s"
      },
      {
        "protocol": "dns",
        "action": "hijack-dns"
      }
    ],
    "rule_set": [
      {
        "type": "local",
        "tag": "geosite-geolocation-cn",
        "format": "binary",
        "path": "/usr/share/vbox/geosite/geosite-geolocation-cn.srs"
      },
      {
        "type": "local",
        "tag": "geosite-geolocation-!cn",
        "format": "binary",
        "path": "/usr/share/vbox/geosite/geosite-geolocation-!cn.srs"
      },
      {
        "type": "local",
        "tag": "geoip-cn",
        "format": "binary",
        "path": "/usr/share/vbox/geoip/geoip-cn.srs"
      },
      {
        "type": "local",
        "tag": "geosite-category-ads-all",
        "format": "binary",
        "path": "/usr/share/vbox/geosite/geosite-category-ads-all.srs"
      }
    ],
    // 默认出站标签。如果为空，将使用第一个可用于对应协议的出站。
    "final": "direct",
    "default_domain_resolver": {
      "server": "dns-local"
      // "rewrite_ttl": 60,
      // "client_subnet": "114.114.114.114/24" // Any China client IP address
      // "client_subnet": "202.96.209.133/24" // Shanghai ChinaNet IP address
      // "client_subnet": "210.22.70.3/24" // Shanghai ChinaUnion IP address
    }
  },
  "experimental": {
    "cache_file": {
      "enabled": true // ,
      // Available from 1.9.0
      // "store_rdrc": true
    }
  }
}
