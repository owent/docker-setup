{
  "log": {
    "disabled": false,
    "level": "info",
    "output": "/var/log/vbox/box.log",
    "timestamp": true
  },
  "dns": {
    // When proxy server support ipv4 only
    "strategy": "prefer_ipv4", // prefer_ipv4,prefer_ipv6,ipv4_only,ipv6_only
    "servers": [
      {
        "tag": "dns-proxy",
        "address": "tls://8.8.8.8",
        "strategy": "ipv4_only"
      },
      // {
      //   "tag": "dns-proxy",
      //   "address": "tls://1.1.1.1"
      // },
      {
        "tag": "dns-local",
        "address": "tls://223.5.5.5",
        "detour": "direct"
      } // ,
      // {
      //   "tag": "dns-local",
      //   "address": "https://223.5.5.5/dns-query",
      //   "detour": "direct"
      // }
    ],
    // 默认使用第一个服务器
    // "final": "",
    "rules": [
      // 自定义服务器
      {
        "domain_suffix": [
          "shkits.com",
          "x-ha.com",
          "r-ci.com",
          "myip.biturl.top",
          "ifconfig.me",
          "ifconfig.io",
          "api.myip.la",
          "getip.icu",
          "ip.threep.top ip.sb",
          "test-ipv6.com"
        ],
        "server": "dns-local"
      },
      {
        "rule_set": "geosite-geolocation-cn",
        "server": "dns-local"
      },
      // client_subnet is available from 1.9.0
      {
        "type": "logical",
        "mode": "and",
        "rules": [
          {
            "rule_set": "geosite-geolocation-!cn",
            "invert": true
          },
          {
            "rule_set": "geoip-cn"
          }
        ],
        "server": "dns-proxy",
        // "client_subnet": "114.114.114.114/24" // Any China client IP address
        // "client_subnet": "202.96.209.133/24" // Shanghai ChinaNet IP address
        "client_subnet": "210.22.70.3/24" // Shanghai ChinaUnion IP address
      },
      {
        "outbound": "any",
        "server": "dns-local"
      }
    ]
  },
  "inbounds": [
    {
      "type": "vmess",
      "tag": "in-vmess",
      "listen": "::",
      "listen_port": 8371,
      "sniff_override_destination": true,
      "sniff": true,
      "tcp_fast_open": false,
      "proxy_protocol": false,
      "users": [
        {
          "name": "<USER NAME>",
          "uuid": "<UUID>",
          "alterId": 256
        }
      ],
      "tls": {
        "enabled": true,
        "server_name": "example.org",
        "insecure": false,
        "alpn": ["h2"],
        "min_version": "1.2",
        "certificate_path": "<CERT/FULLCHAIN FILE PATH>",
        "key_path": "<KEY FILE PATH>"
      },
      "transport": {
        "type": "ws",
        "path": "/redirect",
        "max_early_data": 2048,
        "early_data_header_name": "Sec-WebSocket-Protocol"
      }
    },
    {
      "type": "trojan",
      "tag": "in-trojan",
      "listen": "::",
      "listen_port": 8372,
      "users": [
        {
          "name": "<USER NAME>",
          "password": "<PASSWORD>"
        }
      ],
      "tls": {
        "enabled": true,
        "server_name": "example.org",
        "insecure": false,
        "alpn": ["h2"],
        "min_version": "1.2",
        "certificate_path": "<CERT/FULLCHAIN FILE PATH>",
        "key_path": "<KEY FILE PATH>"
      },
      "fallback": {
        "server": "127.0.0.1",
        "server_port": 8375
      },
      "fallback_for_alpn": {
        "h2": {
          "server": "127.0.0.1",
          "server_port": 8375
        }
      }
    },
    {
      "type": "vless",
      "tag": "in-vless",
      "listen": "::",
      "listen_port": 8373,
      "sniff_override_destination": true,
      "sniff": true,
      "tcp_fast_open": false,
      "proxy_protocol": false,
      "users": [
        {
          "name": "<USER NAME>",
          "uuid": "<UUID>",
          "flow": ""
        }
      ],
      "tls": {
        "enabled": true,
        "server_name": "example.org",
        "insecure": false,
        "alpn": ["h2", "http/1.1"],
        "min_version": "1.2",
        "certificate_path": "<CERT/FULLCHAIN FILE PATH>",
        "key_path": "<KEY FILE PATH>"
      },
      "transport": {
        "type": "ws",
        "path": "/redirect",
        "max_early_data": 2048,
        "early_data_header_name": "Sec-WebSocket-Protocol"
      }
    }
  ],
  "outbounds": [
    {
      "type": "direct",
      "tag": "direct"
    },
    {
      "type": "dns",
      "tag": "dns-out"
    },
    {
      "type": "block",
      "tag": "block"
    }
  ],
  "route": {
    "rules": [
      {
        "protocol": "dns",
        "outbound": "dns-out"
      }
    ],
    "rule_set": [
      {
        "type": "local",
        "tag": "geosite-geolocation-cn",
        "format": "binary",
        "path": "/usr/share/vbox/geosite/geosite-geolocation-cn.srs"
      },
      {
        "type": "local",
        "tag": "geosite-geolocation-!cn",
        "format": "binary",
        "path": "/usr/share/vbox/geosite/geosite-geolocation-!cn.srs"
      },
      {
        "type": "local",
        "tag": "geoip-cn",
        "format": "binary",
        "path": "/usr/share/vbox/geoip/geoip-cn.srs"
      },
      {
        "type": "local",
        "tag": "geosite-category-ads-all",
        "format": "binary",
        "path": "/usr/share/vbox/geosite/geosite-category-ads-all.srs"
      }
    ],
    // 默认出站标签。如果为空，将使用第一个可用于对应协议的出站。
    "final": "direct"
  },
  "experimental": {
    "cache_file": {
      "enabled": true // ,
      // Available from 1.9.0
      // "store_rdrc": true
    }
  }
}
